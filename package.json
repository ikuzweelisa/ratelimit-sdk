{
  "name": "ratelimit-sdk",
  "version": "0.5.2",
  "type": "module",
  "access": "public",
  "author": "IKUZWE Elisa <shemaelyssa@gmail.com>",
  "description": "Generic Rate Limiter for JavaScript/TypeScript",
  "main": "./.dist/index.js",
  "types": "./.dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/ikuzweelisa/ratelimit-sdk"
  },
  "homepage": "https://github.com/ikuzweelisa/ratelimit-sdk",
  "keywords": [
    "ratelimit-js",
    "node-ratelimit",
    "express-ratelimiter",
    "ratelimit-sdk"
  ],
  "bugs": {
    "url": "https://github.com/ikuzweelisa/ratelimit-sdk/issues"
  },
  "scripts": {
    "build": "tsdown",
    "test": "vitest",
    "clean": "rm -rf .dist && rm -rf node_modules",
    "typecheck": "tsc --noEmit",
    "format": "biome format --write",
    "lint": "biome lint --write"
  },
  "devDependencies": {
    "@biomejs/biome": "2.4.0",
    "@types/node": "^24.0.14",
    "typescript": "^5.8.3",
    "vitest": "^4.0.0",
    "tsdown": "^0.20.0"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./.dist/index.d.ts",
      "import": "./.dist/index.js",
      "require": "./.dist/index.cjs",
      "default": "./.dist/index.js"
    },
    "./utils": {
      "types": "./.dist/utils.d.ts",
      "import": "./.dist/utils.js",
      "require": "./.dist/utils.cjs",
      "default": "./.dist/utils.js"
    }
  },
  "files": [
    ".dist/",
    "README.md",
    "package.json",
    "LICENSE"
  ]
}
